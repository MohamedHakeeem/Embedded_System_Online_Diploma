#ifndef KEYPAD_H_
#define KEYPAD_H_

#include <stm32f103x6_gpio_driver.h>
#include <STM32F103X6.h>



#define R0 GPIO_PIN_0
#define R1 GPIO_PIN_1
#define R2 GPIO_PIN_8
#define R3 GPIO_PIN_14

#define C0 GPIO_PIN_11
#define C1 GPIO_PIN_12
#define C2 GPIO_PIN_13
//#define C3 GPIO_PIN_7

#define KEYPAD_PORT GPIOA

void Keypad_init();
char Keypad_getkey();







#endif /* KEYPAD_H_ */
